<div class="jobs-grid">
  <% jobs.forEach(job=> { %>
  <div class="job">
    <img src="<%= job.company_logo %>" />
    <div class="info">
      <object>
        <a href="<%= job.company_url %>"> <%= job.company %> </a>
      </object>
      <h2><%= job.title %></h2>
      <span> <%= job.location %> </span>
    </div>
    <div class="modal">
      <div class="content-modal">
        <img src="<%= job.company_logo %>" />
        <h2><%= job.title %></h2>
        <span> <%= job.location %> </span>
        <p><%- job.description %></p>
        <p><%- job.how_to_apply %></p>
      </div>
    </div>
  </div>
  <% }); %>
</div>
